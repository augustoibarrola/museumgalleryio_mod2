<h1> Discover Page </h1>
<% @users.each do |user| %>
    <% if @current_user != user%>
        <a href=<%= user_path(user) %> >
            <img src="<%= user.profile_picture %>" alt="<%= user.name %> Profile Pic" class="index-profile-picture" width="700" height="600">
        </a>
    <% end %>
<% end %>