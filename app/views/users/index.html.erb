<h1> Discover Page </h1>
<% @users.each do |user| %>
    <% if @current_user != user%>
        <a href=<%= view_other_path(user) %> >
            <img src="<%= (url_for(user.profile_picture)) %>" alt="<%= user.name %> Profile Pic" class="index-profile-picture" width="700" height="600">
        </a>
        <h3><%= user.name %></h3>
    <% end %>
<% end %>