<h1 class="art-bench"> Art Bench </h1>

<div class="row">
    <div class="column">
    <% @users.each do |user| %>
        <% if @current_user != user%>
            <a href=<%= view_other_path(user) %> >
                <div class="indeximage">
                <img src="<%= (url_for(user.profile_picture)) %>" alt="<%= user.name %> Profile Pic" class="index-profile-picture" >
                <h3 class="user-name"><%= user.name %></h3>
                </div>
            </a>
      
        <% end %>
    <% end %>
    </div>
</div>